<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yct.settle.mapper.CardAreaMapper">

    <select id="getIssuesByCardNo" resultType="java.lang.String">
        SELECT AREA_CODE from T_CARD_AREA
        WHERE #{cardNo} between START_NUM and END_NUM
    </select>

</mapper>